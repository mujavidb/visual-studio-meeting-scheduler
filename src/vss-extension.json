{
    "manifestVersion": 1,
    "id": "meeting-extension",
    "version": "0.1.62",
    "name": "Meetings",
    "description": "An extension to handle meeting scheduling and management.",
    "publisher": "mujavidbukhari",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "assets/images/meeting.png"
    },
    "contributions": [
        {
            "id": "VS1TEAM8-Meetings-HubGroup",
            "type": "ms.vss-web.hub-group",
            "description": "Meetings hub group.",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Meetings",
                "order": 100
            }
        },
        {
            "id": "VS1TEAM8-Meetings-HubGroup-DashboardHub",
            "type": "ms.vss-web.hub",
            "description": "Meetings dashboard hub.",
            "targets": [".VS1TEAM8-Meetings-HubGroup"],
            "properties": {
                "name": "Dashboard",
                "order": 1,
                "uri": "index.html"
            }
        }
    ],
    "scopes": [
        "vso.profile",
        "vso.work"
    ],
    "files": [
        {
            "path": "index.html", "addressable": true
        },
        {
            "path": "index.js", "addressable": true
        },
        {
            "path": "assets/js/VSS.SDK.min.js", "addressable": true
        },
        {
            "path": "assets/images/meeting.png", "addressable": true
        },
        {
            "path": "assets/css", "addressable": true
        }
    ]
}
