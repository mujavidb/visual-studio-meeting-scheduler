{
  "name": "visual-studio-meeting-scheduler",
  "description": "A meeting scheduler extension for Microsoft's VSTS platform for organising meetings.",
  "version": "1.0.0",
  "angular-cli": {},
  "scripts": {
    "dev": "npm run angular_server & npm run watch_sass",
    "angular_server": "ng serve",
    "watch_sass": "node-sass --watch --output-style compressed --include-path scss src/scss/main.scss src/public/css/main.css",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "license": "MIT",
  "author": "Mujavid Bukhari, Alasdair Hall and Kelvin Chan",
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "angular2-fullcalendar": "^1.1.1",
    "core-js": "^2.4.1",
    "moment": "^2.17.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "6.13.2",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2015-webpack": "6.4.3",
    "node-sass": "^3.13.1",
    "postcss-cli": "^2.5.2",
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  },
  "babel": {
    "minified": true,
    "shouldPrintComment": false
  }
}
